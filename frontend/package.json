    "mobx": "^6.5.0",
    "mobx-react": "^7.3.0",
    "moment": "^2.29.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.2.1",
    "react-router-dom": "^6.2.1",
