{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production pm2 start ./dist/index.js",
    "stop": "cross-env NODE_ENV=production pm2 stop ./dist/index.js",
    "restart": "yarn stop && yarn build && yarn start",
    "build": "tsc",
    "dev": "cross-env NODE_ENV=development nodemon src/index.ts",
    "dev:server": "cross-env NODE_ENV=production nodemon index.js",
    "oa:delete:all": "cd ./scripts && node clearGeneratedClients.js",
    "oa:generate:fetch": "openapi-generator-cli generate --generator-key fetch",
    "oa:generate:axios": "openapi-generator-cli generate --generator-key axios",
    "oa:generate:docs-confluence": "openapi-generator-cli generate --generator-key docs-confluence",
    "oa:generate:graphql": "openapi-generator-cli generate --generator-key graphql",
    "nswag:generate": "dotnet run --project nswag/APIClientGenerator swaggerSrc/cim.json nswagClients/cim/lib/clients.ts TypeScript",
    "start-prod": "pm2 start -f index.js",
    "clear-prod": "pm2 stop all && pm2 delete all"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "@types/express-fileupload": "^1.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.194",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "express": "^4.18.1",
    "express-fileupload": "^1.4.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "nodemon": "^2.0.19",
    "openapi-schema-validator": "^12.1.0",
    "pm2": "^5.2.2",
    "ws": "^8.8.1",
    "zip-a-folder": "^1.1.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.13",
    "@types/node": "^18.6.2",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
